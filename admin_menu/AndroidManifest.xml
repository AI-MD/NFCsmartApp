<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smart_attbook"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="11" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        
       
        
        <activity
            android:name="com.example.admin_menu.AdminActivity"
            android:label="@string/title_activity_admin__menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
       
        <activity
            android:name="com.example.admin_menu.StudentActivity"
            android:label="@string/title_activity_student" >
        </activity>
        <activity
            android:name="com.example.admin_menu.ProfessorActivity"
            android:label="@string/title_activity_professor" >
        </activity>
        <activity
            android:name="com.example.admin_menu.LectureActivity"
            android:label="@string/title_activity_lecture" >
        </activity>
       
        <activity
            android:name="com.example.admin_menu.StudentEditActivity"
            android:label="@string/title_activity_student_edit" >
        </activity>
        
        <activity
            android:name="com.example.admin_menu.ProfessorEditActivity"
            android:label="@string/title_activity_professor_edit" >
        </activity>
       <activity
            android:name="com.example.admin_menu.LectureEditActivity"
            android:label="@string/title_activity_lecture_edit" >
        </activity>
       
    </application>

</manifest>